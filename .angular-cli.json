{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "app-web-hadif-cli"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon",
        "fonts",
        "app/config.json", 
        "app/i18n/ar.json", 
        "app/i18n/en.json" ,
        "css/main.css",
        "css/english-main.css"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "css/vis.css",
        "css/responsive.css",
        "lib/videojs.ads/videojs.ads.css",
        "css/bootstrap.css",
        "css/font-awesome.css",
        "css/timeslider.css",
        "css/simplebar.css",
        "css/jquery.scrollbar.css",
        "css/bootstrap-rating.css",
        "css/jquery.fancybox.css", 
        "../node_modules/angular2-toaster/toaster.css",
        "../node_modules/video.js/dist/video-js.css"
      ],
      "scripts": [
        "js/jquery-1.12.4.js",
        "js/jquery-ui.min.js",
        "js/bootstrap.js",
        "js/timeslider.js",
        "js/simplebar.min.js",
        "js/jquery.scrollbar.js",
        "js/jquery.payment.js",
        "js/bootstrap-rating.js",
        "js/jquery.fancybox.js",
        "js/fancy-controller.js",
        "js/main.js",
        "../node_modules/video.js/dist/video.min.js",
        "../node_modules/videojs-contrib-hls/dist/videojs-contrib-hls.min.js",
        "js/vis.min.js",
        "lib/videojs-preroll/videojs-preroll.js",
        "lib/videojs.ads/videojs.ads.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
